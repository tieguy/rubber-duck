# Rubber Duck Environment Variables
# Copy to .envrc and fill in values (never commit actual secrets)

# Discord
export DISCORD_BOT_TOKEN="your-bot-token"
export DISCORD_OWNER_ID="your-discord-user-id"

# Anthropic (Claude)
export ANTHROPIC_API_KEY="your-anthropic-key"

# Todoist
export TODOIST_API_KEY="your-todoist-key"

# Letta Cloud
export LETTA_API_KEY="your-letta-key"

# Google Calendar (optional - service account)
# Base64-encode your service account JSON key:
#   base64 -w0 service-account.json
# Then share your calendar with the service account email
export GOOGLE_SERVICE_ACCOUNT_JSON="base64-encoded-json-key"
